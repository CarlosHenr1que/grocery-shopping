{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["jest", "node"]
  },
  "include": [
     "src/**/*",
    "**/*.test.ts"
  ],
  "exclude": ["node_modules", "dist",  "**/*.test.ts", "**/*.test.js"],
}
